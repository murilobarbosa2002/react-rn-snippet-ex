{
    "React Native Route": {
    "prefix": "rnr",
    "body": [
      "import ${1:ScreenName}Screen from '';",
      "",
      "export default function ${1:ScreenName}Route() {",
      "    return <${1:ScreenName}Screen />;",
      "}"
    ],
    "description": "Cria uma rota de tela em React Native com o padrão de nomenclatura."
  },
  "React Native Component Export": {
    "prefix": "rnc",
    "body": [
      "import { View, Text } from 'react-native';",
      "",
      "const ${1:NameComponent}Component = () => {",
      "  return (",
      "    <View>",
      "      <Text>${1:NameComponent}Component</Text>",
      "    </View>",
      "  );",
      "};",
      "",
      "export default ${1:NameComponent}Component;"
    ],
    "description": "Cria um componente básico React Native com exportação"
  },
  "React Native Component Export with Types": {
    "prefix": "rncwt",
    "body": [
      "import React from 'react';",
      "import { View, Text } from 'react-native';",
      "",
      "interface ${1:NameComponent}Props {",
      "  name: string;",
      "}",
      "",
      "const ${1:NameComponent}Component: React.FC<${1:NameComponent}Props> = ({ props }) => {",
      "  return (",
      "    <View>",
      "      <Text>{props.name}</Text>",
      "    </View>",
      "  );",
      "};",
      "",
      "export default ${1:NameComponent}Component;"
    ],
    "description": "Cria um componente React Native com tipos TypeScript e props"
  },
